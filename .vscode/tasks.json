{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Rust: Build Debug",
			"command": "cargo",
			"type": "process",
			"args": [
				"build",
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": "build"
		},
		{
			"label": "Rust: Build Release",
			"command": "cargo",
			"type": "process",
			"args": [
				"build",
				"--release"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": "build"
		}
	]
}